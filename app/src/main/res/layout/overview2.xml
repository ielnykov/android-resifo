                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/c_postalCode" />
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" >

                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:ems="10"
                        android:id="@+id/editCPostalCode" />
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" >

                    <TextView
                        android:text="City/town, federal province"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/c_city" />
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" >

                    <EditText
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:ems="10"
                        android:id="@+id/editCCity" />
                </TableRow>


                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" >

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:text="Are you moving abroad"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/textView26"
                            android:layout_weight="1" />

                        <TextView
                            android:text="no"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/textVMAbroad"
                            android:layout_weight="1"
                            android:textAlignment="textEnd" />
                    </LinearLayout>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" >

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <Button
                            android:text="Back"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/button14"
                            android:layout_weight="1"
                            android:backgroundTint="@color/colorPrimary"
                            android:textColor="@android:color/white"
                            android:onClick="backOV" />

                        <Button
                            android:text="Done"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/button13"
                            android:layout_weight="1"
                            android:backgroundTint="@color/colorPrimary"
                            android:textColor="@android:color/white"
                            android:onClick="doneOV"/>
                    </LinearLayout>
                </TableRow>
            </TableLayout>
        </ScrollView>
    </LinearLayout>
</android.support.v4.widget.NestedScrollView>